<Window x:Class="WPFRoman.View.Windows.SheduleWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:schedule="clr-namespace:Syncfusion.UI.Xaml.Schedule;assembly=Syncfusion.SfSchedule.WPF"
        xmlns:local="clr-namespace:WPFRoman.View.Windows"
        mc:Ignorable="d"
        Title="SheduleWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="561*"/>
            <ColumnDefinition Width="393*"/>
        </Grid.ColumnDefinitions>
        <schedule:SfSchedule x:Name="DoctorsShedule" ScheduleType="Month" Grid.ColumnSpan="2"/>
        <TextBlock x:Name="DoctorNameOnShedule"
                   Grid.Column="1"
                   FontSize="19"
                   FontWeight="Light"
                   Padding="20,10"
                   HorizontalAlignment="Right">DOCTOR NAME</TextBlock>
    </Grid>
</Window>
